{
  "name": "asset-module-webpack-plugin",
  "version": "0.0.1",
  "description": "A webpack plugin that emits JS modules for your assets such as images. This allows Node to load the modules that webpack generated for your assets without having to run webpack's bundle on the server.",
  "main": "lib/AssetModulePlugin.js",
  "scripts": {
    "build": "babel --out-dir lib src"
  },
  "engines": {
    "node": ">=2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exponentjs/asset-module-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "webpack-plugin",
    "assets",
    "server-side-rendering"
  ],
  "author": "James Ide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exponentjs/asset-module-webpack-plugin/issues"
  },
  "homepage": "https://github.com/exponentjs/asset-module-webpack-plugin#readme",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-eslint": "^3.1.14",
    "eslint": "^0.22.1"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
